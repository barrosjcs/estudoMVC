<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title Quiron Administrativo</title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.css" rel="stylesheet" />
    <link href="~/Content/personalizado.css" rel="stylesheet" />

    <script src="~/scripts/jquery-1.10.2.js"></script>
    <script src="~/scripts/bootstrap.min.js"></script>
    <script src="~/scripts/jquery.validate.js"></script>
    <script src="~/scripts/jquery.validate.unobtrusive.js"></script>
    <script src="~/scripts/jquery.fixes.js"></script>
</head>
<body>
    <div>
        @if(TempData["mensagem"] != null)
        {
            <div class="alert alert-success" id="mensagem">@TempData["mensagem"]</div>
        }

        @RenderBody()
    </div>
</body>
</html>